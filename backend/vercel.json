{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 60,
        "includeFiles": [
          "../CONSOLIDATED_OUTPUT_DATA.csv",
          "../db_summary.json",
          "../context_kpi_mapping.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
  
}